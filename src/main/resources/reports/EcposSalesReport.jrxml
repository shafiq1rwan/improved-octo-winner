	<queryString language="SQL">
		<![CDATA[SELECT c.name, COUNT(c.name) as qty, (c.detail_item_price*count(c.name)) as ttl 
					FROM checks b JOIN details c ON b.chk_seq = c.chk_seq
					WHERE c.detail_item_status = 0 
					AND c.detail_type = 'S' AND b.chk_open = 3
					AND month(b.createdate) >= $P{month}  AND year(b.createdate)= $P{year}
					GROUP BY c.name ORDER BY b.chk_seq]]>
	</queryString>
